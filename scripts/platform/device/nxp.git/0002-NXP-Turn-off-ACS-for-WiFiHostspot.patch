From 7adfada05817d0e3e0fe7b75b16e9b8a5b72f5ee Mon Sep 17 00:00:00 2001
From: mohit <mohit.p@variscite.com>
Date: Thu, 26 Aug 2021 11:34:52 -0700
Subject: [PATCH] NXP: Turn off ACS for WiFiHostspot

---
 common/wifi/WifiOverlay/res/values/config.xml | 6 +++---
 1 file changed, 3 insertions(+), 3 deletions(-)

diff --git a/common/wifi/WifiOverlay/res/values/config.xml b/common/wifi/WifiOverlay/res/values/config.xml
index 42d59ebd..635069cd 100644
--- a/common/wifi/WifiOverlay/res/values/config.xml
+++ b/common/wifi/WifiOverlay/res/values/config.xml
@@ -27,10 +27,10 @@
     <bool translatable="false" name="config_wifi_background_scan_support">true</bool>
 
     <!-- Enable ACS (auto channel selection) for Wifi hotspot (SAP) -->
-    <bool translatable="false" name="config_wifi_softap_acs_supported">true</bool>
+    <bool translatable="false" name="config_wifi_softap_acs_supported">false</bool>
 
     <!-- Enable 802.11ac for Wifi hotspot (SAP) -->
-    <bool translatable="false" name="config_wifi_softap_ieee80211ac_supported">true</bool>
+    <bool translatable="false" name="config_wifi_softap_ieee80211ac_supported">false</bool>
 
     <!-- True if the firmware supports connected MAC randomization -->
     <bool name="config_wifi_connected_mac_randomization_supported">true</bool>
@@ -39,5 +39,5 @@
     <bool name="config_wifi_p2p_mac_randomization_supported">true</bool>
 
     <!-- True if the firmware supports ap MAC randomization -->
-    <bool name="config_wifi_ap_mac_randomization_supported">true</bool>
+    <bool name="config_wifi_ap_mac_randomization_supported">false</bool>
 </resources>
